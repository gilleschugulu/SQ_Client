<div id='equipe-btn'></div>
<div id='date'>EDITION DU {{date}}</div>
<div id='title' class='resize'>{{winner}} a mis sa patee a {{loser}}</div>
<div id='vs'>
  {{#each participants}}
  <div class='participant'>
      <div class='photo'
      {{#if fb_id}}
        style='background-image:url(https://graph.facebook.com/{{fb_id}}/picture?width=69&height=69)'
      {{/if}}
      ></div>
    <div class='blaze resize'>{{username}}</div>
    <div class='score'>{{score}}</div>
  </div>
  {{/each}}
</div>
<div id='buttons'>
  <a id='invite-btn'></a>
  <a id='ranking-btn' href="#hall-of-fame/home"></a>
</div>